{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "lib": ["ES5", "ES6", "DOM", "ES2015", "ES2016", "ES2020", "ES2019"],
    "target": "ES6"
  },
  "include": ["*.ts"],
  "exclude": ["*.js"]
}

